{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "NAI Tag Classifier",
  "version": "0.1.0",
  "identifier": "com.nai.tag.classifier",
  "build": {
    "devUrl": "http://localhost:5173",
    "frontendDist": "../dist"
  },
  "app": {
    "security": {
      "capabilities": [
        "main-window"
      ],
      "assetProtocol": {
        "enable": true,
        "scope": [
          "**/*"
        ]
      }
    },
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "NAI Tag Classifier",
        "width": 1200,
        "height": 800,
        "devtools": true
      }
    ]
  }
}
